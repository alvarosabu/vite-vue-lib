  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.2.0",
    "pathe": "^1.0.0",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vite-plugin-banner": "^0.6.1",
    "vite-plugin-dts": "^1.7.1",
    "vue-tsc": "^1.0.9"
