{
  "name": "vite-vue-lib-template",
  "private": true,
  "version": "0.0.0",
  "author": "Alvaro Saburido <hola@alvarosaburido.dev> (https://github.com/alvarosabu/)",
  "type": "module",
  "main": "./dist/vite-vue-lib.umd.cjs",
